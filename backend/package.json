{
  "name": "crm-codex-backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:generate": "prisma generate --schema ../prisma/schema.prisma",
